
public class hw01 {
    public static void main(String[] args) {
      Student student1=new Student();
      student1.setName("Roy");
      System.out.println(student1.getName());
      student1.setNumber("19");
      System.out.println(student1.getNumber());
      student1.setId("108021089");
      System.out.println(student1.getId());
      student1.setYear("15");
      System.out.println(student1.getYear());
      student1.setGrade("86");
      System.out.println(student1.getGrade());
      student1.showInfo("86");
      System.out.println(student1.show());
        
       
    }
}
/////////////////////////Student.java
public class Student{
    private String name,number,id,year,grade,showinfo;
    void setName(String setname){
        this.name=setname;
    }
    String getName(){
        return name;
    }


    void setNumber(String setnumber){
        this.number=setnumber;
    }
    String getNumber(){
        return number;
    }



    void setId(String setid){
        this.id=setid;
    }
    String getId(){
        return id;
    }



    void setYear(String setyear){
        this.year=setyear;
    }
    String getYear(){
        return year;
    }


    void setGrade(String setgrade){
        this.grade=setgrade;
    }
    String getGrade(){
        return grade;
    }
    void showInfo(String showinfo){
        this.showinfo=showinfo;
    }
    String show(){
        return name+" "+number+" "+id+" "+year+" "+grade;
        
    }
  

}
